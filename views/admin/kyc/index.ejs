              if(user.user_role!="admin"&& user.image){%>
               
              <tr>
                <td><%- index+1 %></td>
               
                <td><%- user.name %></td>
                <td><%- user.email %></td>
                <td></td>
                <td></td>

                <td class="success-color"><%- user.status %></td>
                
                 
                <td>
                  <div class="edit-icons">
                    <a href="/users/view-kyc?id=<%=user._id %>" data-toggle="tooltip" data-placement="top" title="Edit"><i class="mdi mdi-eye"></i></a>
                   
                  </div>
                </td>
               
              </tr>

              <% } }) %>


            </tbody>
          </table>
        </div>
