            });
                
           //$("#content-list").html(html); 
             }else{
              document.getElementById('content-list').innerHTML='<div class="col-md-6"> <h1>Not Found<h1> </div>';  


             } 
                   


             },
         
      });
    }
 
  function CheckboxValues(name) {
    const checkboxes = document.querySelectorAll(`input[name="${name}"]:checked`);
    let values = [];
    let basic_price=[];
    checkboxes.forEach((checkbox) => { alert(checkbox.value)
       
      if(name="category"){
          values.push(checkbox.value);
         }else
          {
            basic_price.push(checkbox.value);
          }
      
     });
   
     console.log(values);
     console.log(basic_price);
     fetchData(values,basic_price);
   }   

   
  </script>