    function fetchData(query,basic_price){   //console.log(query)
      $.ajax({type:"get",
             url:"/users/serach-content",
             data:{category:query,basic_price:basic_price},
             success:function(res)
             {

              if(res.length>0)
                {               
                  
                  document.getElementById('content-list').innerHTML="";  

                  res.forEach(function(content,index)
                   { let html="";
                       if(content.media_type[0]=="video"){

                         html='<div class="col-md-6">\
                          <div class="info-box-certify multiple-certify-inner">\
                <div class="row align-item-center">\
                   <div class="col-xl-4 offset-xl-1 col-md-12">\
                     <div>\
                      <video width="200"height="200">\
                        <source src="/uploadFile/'+content.image+'" type="video/'+content.media_type[1]+'">\
                      </video>\
                     </div>\
                   </div>\
                   <div class="col-xl-6 col-md-12">\
                     <table class="table">\
                       <tbody>\
                         <tr>\
                           <td width="40%">Category</td>\
                           <td width="10%">:</td>\
                           <td width="40%">'+content.category+'</td>\
                         </tr>\
                        <tr>\
                           <td>Cotent</td>\
                           <td>:</td>\
                           <td>'+content.title+'</td>\
                         </tr>\
                         <tr>\
                           <td>Basic Price</td>\
                           <td>:</td>\
                           <td>'+content.basic_price+'</td>\
                         </tr>\
                         <tr>\
                           <td>For Sale</td>\
                           <td>:</td>\
                           <td>'+content.for_sale+'</td>\
                         </tr>\
                         <tr>\
                           <td>Image Type</td>\
                           <td>:</td>\
                           <td>'+content.media_type[1]+'</td>\
                         </tr>\
                       </tbody>\
                     </table>\
                     <div>\
                        <h6 class="pending-line"><b>'+content.status+'</b> &nbsp; '+content.created_at+'</h6>\
                     </div>\
                   </div>\
                 </div>\
             </div>';

                       }
                       else
                       {

      
                           html='<div class="col-md-6">\
                          <div class="info-box-certify multiple-certify-inner">\
                <div class="row align-item-center">\
                   <div class="col-xl-4 offset-xl-1 col-md-12">\
                     <div>\
                       <img src="/uploadFile/'+content.image+'" alt="artist img" class="img-fluid"/>\
                     </div>\
                   </div>\
                   <div class="col-xl-6 col-md-12">\
                     <table class="table">\
                       <tbody>\
                         <tr>\
